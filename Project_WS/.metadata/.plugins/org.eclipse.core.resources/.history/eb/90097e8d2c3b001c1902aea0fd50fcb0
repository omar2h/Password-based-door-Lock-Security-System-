#include "App.h"
#include "../ECUAL/keypad.h"


void App_init(){
	UART_ConfigType UART_Configurations = {BAUDRATE_9600, DATA_8BITS, NO_PARITY, ONE_STOP_BIT};

	UART_init(&UART_Configurations);
	LCD_init();
	App_start();

}

void App_start(){
	while(1){
		LCD_displayString("Please enter new password");
		LCD_moveCursor(1,1);
	}
}
